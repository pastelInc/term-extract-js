{
  "name": "term-extract-js",
  "version": "2.0.1-alpha.3",
  "description": "JavaScript implementation of TermExtract",
  "main": "lib/term-extract.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps inline",
    "watch": "babel src --out-dir lib --watch --source-maps inline",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pastelInc/term-extract-js.git"
  },
  "keywords": [
    "mecab"
  ],
  "author": "pastelInc",
  "license": "MIT",
  "engines": {
    "node": ">=6.10.0",
    "npm": ">=3.10.7"
  },
  "bugs": {
    "url": "https://github.com/pastelInc/term-extract-js/issues"
  },
  "ava": {
    "files": [
      "test/*-test.js"
    ],
    "require": "babel-register",
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "testdouble": "^3.7.0"
  },
  "dependencies": {
    "mecab-async": "^0.1.2"
  }
}
